def check_palindrome(s):
    # Complete the function
    t = s[::-1]
    if s == t:
        return True
    else:
        return False
        
def main():
    test_cases = int(input())
    # Complete the function
    while test_cases > 0:
        s = input()
        if check_palindrome(s) == True:
            print('YES')
        else:
            print('NO')
        test_cases -= 1
main()
