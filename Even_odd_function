def is_even(num):
    # Complete the function 
     return num%2 == 0
def main():
    # Complete the function 
    t = int(input())
    for i in range (t):
        num = int(input())
        if is_even(num):
            print('Even')
        else:
            print('Odd')
            
if __name__ == "__main__":
    main()
